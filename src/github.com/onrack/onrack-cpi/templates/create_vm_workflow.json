{
    "friendlyName": "CF CreateReserve VM",
    "injectableName": "Graph.CF.CreateReserveVM",
    "options": {
      "defaults": {
        "agentSettingsFile": null,
        "agentSettingsPath": null,
        "cid": null,
        "downloadDir": "/opt/downloads",
        "registrySettingsFile": null,
        "registrySettingsPath": null,
        "stemcellFile": null
      },
      "bootstrap-ubuntu": {
        "overlayfs": "common/overlayfs_all_files.cf.cpio.gz"
      }
    },
    "tasks": [
      {
        "label": "set-boot-pxe",
        "taskName": "Task.Obm.Node.PxeBoot",
        "ignoreFailure": true
      },
      {
        "label": "reboot",
        "taskName": "Task.Obm.Node.Reboot",
        "waitOn": {
          "set-boot-pxe": "finished"
        }
      },
      {
        "label": "bootstrap-ubuntu",
        "taskName": "Task.Linux.Bootstrap.Ubuntu",
        "waitOn": {
          "reboot": "succeeded"
        }
      },
      {
        "label": "reserve-node",
        "taskName": "Task.Os.Reserve.CF.VM",
        "waitOn": {
          "bootstrap-ubuntu": "succeeded"
        }
      },
      {
        "label": "provision-node",
        "taskName": "Task.Os.Install.CF.Stemcell",
        "waitOn": {
          "reserve-node": "succeeded"
        }
      },
      {
        "label": "shell-reboot",
        "taskName": "Task.ProcShellReboot",
        "waitOn": {
          "provision-node": "finished"
        }
      }
    ]
}
